{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/src",
                "/usr/local/include",
                "/opt/homebrew/include"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/src",
                    "/usr/local/include",
                    "/opt/homebrew/include"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "defines": [],
            "compilerPath": "/opt/homebrew/bin/aarch64-apple-darwin24-g++-14",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "macFrameworkPath": [
                "/Library/Developer/CommandLineTools/SDKs/MacOSX15.sdk/System/Library/Frameworks"
            ],
            "intelliSenseMode": "macos-gcc-arm64",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}